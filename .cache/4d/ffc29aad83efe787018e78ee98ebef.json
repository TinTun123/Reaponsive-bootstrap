ody {\r\n  height: auto;\r\n}\r\n\r\n/* Navigation bar start */\r\n.navbar1 {\r\n  position: absolute;\r\n  top: 0;\r\n  position: fixed;\r\n  width: 100%;\r\n  z-index: 99;\r\n}\r\n\r\n.navbar1 .nav1 {\r\n  display: flex;\r\n  background-color: rgba(56, 62, 76, 0.5);\r\n  justify-content: center;\r\n}\r\n\r\n.navbar1 .nav1 li {\r\n  margin: 0.5rem 1rem;\r\n  text-align: center;\r\n}\r\n\r\n.navbar1 .nav1 li a {\r\n  display: block;\r\n  border: 1px solid rgba(255, 255, 255, 0.3);\r\n  padding: 0.5rem 1.5rem;\r\n  color: aliceblue;\r\n  font-family: \"Open Sans\", Helvetica, Arial, sans-serif;\r\n  font-size: 20px;\r\n}\r\n\r\n.navbar1 .nav1 li a:hover,\r\n.navbar1 .nav1 li a.active {\r\n  background-color: rgba(255, 255, 255, 0.1);\r\n}\r\n\r\n/* Sandwhich icon display */\r\n\r\n.bi-stack {\r\n  z-index: 1000;\r\n}\r\n\r\n.bi-stack {\r\n  color: rgba(255, 255, 255, 1);\r\n  background-color: rgba(255, 255, 255, 0.1);\r\n  font-size: 1.3rem;\r\n  padding: 0.3rem;\r\n  border-radius: 0.6rem;\r\n  position: absolute;\r\n  top: 13px;\r\n  right: 8px;\r\n  position: fixed;\r\n  display: none;\r\n}\r\n\r\n/* Sandwhich icon end */\r\n\r\n/* Responsive Navbar start */\r\n\r\n@media screen and (max-width: 843px) {\r\n  /* .navbar1 {\r\n    display: none;\r\n  } */\r\n\r\n  .navbar1 .small-screen {\r\n    display: block;\r\n    max-height: 47px;\r\n    transition: max-height 0.6s ease-out;\r\n    overflow: hidden;\r\n  }\r\n\r\n  .navbar1 .small-screen.toggle {\r\n    max-height: 650px;\r\n  }\r\n\r\n  .navbar1 .small-screen li {\r\n    margin: 0;\r\n    width: 100%;\r\n    display: block;\r\n    border-bottom: 1px solid rgba(255, 255, 255, 0.3);\r\n    text-align: start;\r\n  }\r\n\r\n  .navbar1 .small-screen li a {\r\n    padding: 1rem 0.7rem;\r\n    border: none;\r\n    text-align: start;\r\n  }\r\n\r\n  .navbar1 .small-screen li a:not(:first-child) {\r\n    padding: 0.5rem 0.7rem;\r\n  }\r\n\r\n  .bi-stack {\r\n    display: block;\r\n  }\r\n}\r\n\r\n/* navbar end */\r\n\r\n.box1,\r\n.box2,\r\n.box3 {\r\n  height: 400px;\r\n}\r\n\r\n/* Header section start */\r\n\r\n.box1 {\r\n  color: white;\r\n}\r\n\r\n.box1 div {\r\n  text-align: center;\r\n  padding-top: 100px;\r\n}\r\n\r\n.box1 div h1 {\r\n  font-size: 70px;\r\n  text-shadow: 2px 2px #000;\r\n}\r\n\r\n.box1 div h4 {\r\n  font-size: 25px;\r\n  text-shadow: 1px 1px #333;\r\n}\r\n\r\n/* Header section end */\r\n\r\n/* Section -02 start */\r\n\r\n/* feature-area start */\r\n\r\n#feature-area {\r\n  height: auto;\r\n  width: auto;\r\n  padding: 7rem 0;\r\n}\r\n\r\n#feature-area div {\r\n  width: 360px;\r\n  margin: auto;\r\n  padding-bottom: 2rem;\r\n  background-color: #f2f2f2;\r\n  text-align: center;\r\n}\r\n#feature-area div img {\r\n  width: 100%;\r\n}\r\n\r\n#feature-area div h2 {\r\n  font-size: 2.5rem;\r\n  font-weight: bolder;\r\n  color: #178a27;\r\n  padding: 0.7rem 2rem;\r\n  text-align: center;\r\n  letter-spacing: 0;\r\n}\r\n\r\n#feature-area div p {\r\n  text-align: center;\r\n  padding: 0 20px 35px;\r\n  color: #333333;\r\n  margin-top: 10px;\r\n  font-size: 14px;\r\n  line-height: 1.6em;\r\n}\r\n\r\n#feature-area div a,\r\n#blog-area .blog-content a {\r\n  color: white;\r\n  background-color: #449d44;\r\n  border-radius: 5px;\r\n  padding: 1rem 3rem;\r\n  font-size: 18px;\r\n}\r\n\r\n#feature-area div:not(:first-child) {\r\n  margin-top: 3rem;\r\n}\r\n\r\n/* feature-area end */\r\n\r\n/* blog content start */\r\n\r\n#blog-area .blog-content {\r\n  width: 555px;\r\n  text-align: center;\r\n  margin: auto;\r\n  background-color: #f2f2f2;\r\n  padding-bottom: 2rem;\r\n}\r\n\r\n#blog-area .blog-content img {\r\n  width: 100%;\r\n}\r\n\r\n#blog-area .blog-content:not(:first-child) {\r\n  margin-top: 3rem;\r\n}\r\n\r\n#blog-area .blog-content h2 {\r\n  color: #666666;\r\n  font-size: 2rem;\r\n  font-weight: 700;\r\n  margin-top: 2rem;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n#blog-area .blog-content p {\r\n  padding: 27px;\r\n  color: #333333;\r\n  font-size: 14px;\r\n  line-height: 1.6em;\r\n}\r\n\r\n/* section - 02 end */\r\n\r\n/* Responsive section - 02 start */\r\n\r\n@media screen and (min-width: 843px) {\r\n  #feature-area {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n\r\n  #feature-area div:not(:first-child) {\r\n    margin: 0;\r\n    padding: 0;\r\n  }\r\n  #feature-area div:first-child,\r\n  #feature-area div:last-child {\r\n    margin-left: 2rem;\r\n    margin-right: 2rem;\r\n  }\r\n\r\n  #blog-area {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n\r\n  #blog-area .blog-content {\r\n    margin: 0 2rem;\r\n    flex: 45%;\r\n  }\r\n  #blog-area .blog-content:nth-child(2) {\r\n    margin-top: 0;\r\n  }\r\n}\r\n\r\n/* Responsive blog & feature section end */\r\n\r\n/* Service section start */\r\n\r\n#service-area {\r\n  text-align: center;\r\n  width: 100%;\r\n  height: 316px;\r\n  color: white;\r\n  margin-top: 3rem;\r\n}\r\n\r\n#service-area h1 {\r\n  text-shadow: 2px 2px #aaa;\r\n  margin-bottom: 20px;\r\n  font-size: 36px;\r\n  font-weight: 500;\r\n  line-height: 1.1;\r\n  padding-top: 80px;\r\n}\r\n\r\n#service-area p {\r\n  font-size: 18px;\r\n  line-height: 1.6em;\r\n}\r\n\r\n/* service area end here */\r\n\r\n/* testimorial-area start */\r\n\r\n.testimorial-area {\r\n  margin-top: 60px;\r\n}\r\n\r\n.testimorial-area .card {\r\n  background-color: #f2f2f2;\r\n  border: none;\r\n  max-width: 350px;\r\n  margin: 3rem auto;\r\n}\r\n\r\n.testimorial-area h5 {\r\n  color: #666666;\r\n  font-size: 20px;\r\n  font-weight: 700;\r\n}\r\n\r\n.testimorial-area p {\r\n  padding: 10px 20px;\r\n  text-align: left;\r\n  font-size: 14px;\r\n  line-height: 1.6em;\r\n}\r\n\r\n.testimorial-area a:hover,\r\n.testimorial-area a:focus {\r\n  background: #e6e6e6 none repeat scroll 0 0;\r\n  color: #666666;\r\n}\r\n\r\n.testimorial-area a:hover {\r\n  text-decoration: none;\r\n  outline: 0;\r\n}\r\n\r\n.testimorial-area a {\r\n  padding: 1rem;\r\n  font-size: 2rem;\r\n  border-radius: 2rem;\r\n}\r\n"},"lineCo